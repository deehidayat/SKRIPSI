TblDosenMengajar	:
SELECT DISTINCT d.id_dosen
FROM dosen d
JOIN dosenjurusan dj ON dj.id_dosen=d.id_dosen
JOIN kegiatan k ON k.id_dosenjurusan=dj.id_dosenjurusan
JOIN matakuliah mk ON mk.id_matakuliah=k.id_matakuliah
JOIN semester s ON s.semester=mk.semester
ORDER BY id_dosen ASC

TblKelasDiajar		:
SELECT DISTINCT j.id_jurusan,mk.semester,k.kelas
FROM jurusan j
JOIN matakuliah mk ON mk.id_jurusan=j.id_jurusan
JOIN kegiatan k ON k.id_matakuliah = mk.id_matakuliah
JOIN semester s ON s.semester=mk.semester
ORDER BY id_jurusan ASC,semester ASC, kelas ASC


